  "bugs": {
    "url": "https://github.com/kei-g/iterators/issues"
  },
  "description": "Iterators Library",
  "devDependencies": {
    "@types/node": "^16.7.4",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "terser": "^5.7.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14.17.5"
  },
  "files": [
    "CHANGELOG.md",
    "index.js",
    "index.d.ts",
    "lib"
  ],
  "homepage": "https://github.com/kei-g/iterators",
  "keywords": [
    "async",
    "asynciterable",
    "asynciterator",
    "iterable",
    "iterator"
  ],
  "license": "BSD-3-Clause",
  "main": "index.js",
  "name": "@kei-g/iterators",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kei-g/iterators.git"
  },
  "scripts": {
    "build": "npm-run-all -p clean lint -p build:mkdir build:tsc -p build:terse:*",
    "build:mkdir": "mkdirp lib",
    "build:terse:circular": "terser build/lib/circular.js -c -m -o lib/circular.js --toplevel",
    "build:terse:concat": "terser build/lib/concat.js -c -m -o lib/concat.js --toplevel",
    "build:terse:empty": "terser build/lib/empty.js -c -m -o lib/empty.js --toplevel",
    "build:terse:index": "terser build/index.js -c -m -o index.js --toplevel",
    "build:terse:iterators": "terser build/lib/iterators.js -c -m -o lib/iterators.js --toplevel",
    "build:terse:linear-number-series": "terser build/lib/linear-number-series.js -c -m -o lib/linear-number-series.js --toplevel",
    "build:terse:multiplex": "terser build/lib/multiplex.js -c -m -o lib/multiplex.js --toplevel",
    "build:terse:single": "terser build/lib/single.js -c -m -o lib/single.js --toplevel",
    "build:terse:transform": "terser build/lib/transform.js -c -m -o lib/transform.js --toplevel",
    "build:terse:zip": "terser build/lib/zip.js -c -m -o lib/zip.js --toplevel",
    "build:tsc": "tsc",
    "clean": "rimraf build/ index.d.ts index.js lib/",
    "lint": "eslint src/lib/*.ts",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s build",
    "test": "ts-node test.ts"
  },
  "types": "index.d.ts",
  "version": "1.2.0"
}
