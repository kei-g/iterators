{
  "compilerOptions": {
    "charset": "utf8",
    "declaration": true,
    "declarationDir": ".",
    "downlevelIteration": true,
    "lib": [
      "ES2015"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "newLine": "lf",
    "outDir": "build",
    "pretty": true,
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "ES6",
  },
  "files": [
    "src/index.ts",
    "src/lib/circular.ts",
    "src/lib/concat.ts",
    "src/lib/empty.ts",
    "src/lib/iterators.ts",
    "src/lib/single.ts",
    "src/lib/zip.ts",
  ],
}
